{
  "name": "tireinformation",
  "version": "0.0.3",
  "description": "A tire information app",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "export": "next export",
    "start": "next start",
    "test": "jest"
  },
  "keywords": [
    "Next.js",
    "Jest",
    "Tire",
    "Information"
  ],
  "author": "4252 Concepts LLC",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!babel.config.js",
      "!next.config.js",
      "!pages/_*.js",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "lcov",
      "cobertura"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    }
  },
  "jest-junit": {
    "suiteName": "NextJS Demo Tests",
    "outputDirectory": "./artifacts",
    "outputName": "junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "^4.5.3",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^9.5.5",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^8.0.0",
    "react-test-renderer": "^16.14.0",
    "ts-jest": "^24.3.0"
  }
}
